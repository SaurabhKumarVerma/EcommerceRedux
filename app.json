{
  "expo": {
    "name": "Ecommerce",
    "slug": "Ecommerce",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/appicon/ecom.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "splash": "./assets/images/splash.png",
      "bundleIdentifier": "com.local.ecommerce"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/appicon/ecom.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.local.ecommerce"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
